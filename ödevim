meme_dict = {
            "CRINGE": "Garip ya da utandırıcı bir şey",
            "LOL": "Komik bir şeye verilen cevap",
            }
word = input("Anlamadığınız bir kelime yazın (hepsini büyük harflerle yazın!) bakalım doğru u:) ")

if word in meme_dict.keys():
    print("yazdığınız kelime doğru")
else:
   print("malesef kelime yanlış :(  ")

